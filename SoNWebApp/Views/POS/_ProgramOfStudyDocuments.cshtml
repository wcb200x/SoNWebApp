@model SoNWebApp.Models.Student

@if (Model.ProgramID == 1)
{
    <img src="~/Content/Images/Traditional BSN POS 002.png" width="1480" height="1433" alt="Traditional BSN Program of Study" />

}

else if (Model.ProgramID == 2)
{

    <img src="~/Content/Images/RN-BSN POS 002.png" width="980" height="1433" alt="RN-BSN Program of Study" />
}

else if (Model.ProgramID == 3)
{
    <img src="~/Content/Images/MEPN POS 01.png" width="1480" height="1633" alt="MEPN Program of Study" />
}

else
{
    <img src="~/Content/Images/Traditional BSN POS 002.png" width="1480" height="1433" alt="Traditional BSN Program of Study" />
}